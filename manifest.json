{
  "manifest_version": 2,
  "name": "Mental Health Mood-lifter",
  "version": "1",

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [ "content.js"]
    }
  ],

  "browser_action": {
      "default_title": "My Bookmarks",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  }



}