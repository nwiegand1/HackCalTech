{
  "manifest_version": 2,
  "name": "Mental Health Mood-lifter",
  "version": "1",

  "content_scripts": [
    {
      "matches": ["<all_urls>", "http://www.google.com/*"],
      "js": [ "tingle.js", "jquery-3.3.1.js", "content.js" ],
      "css": ["tingle.css", "banner.css"]
    }
  ],

  "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },

  "permissions": [
        "activeTab",
        "tabs", "http://www.google.com/*"
    ]

}